<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:elevation="@dimen/elev_navbar"
    tools:context=".activities.ViewActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_view"
        style="@style/Toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:scrollbars="none"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_view">

        <LinearLayout
            android:id="@+id/linearLayout_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView_primary"
                    style="@style/Headline"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="32dp"
                    android:fontFamily="sans-serif"
                    android:text="Route"
                    android:textAllCaps="false"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView_routeVar"
                    style="@style/Heading1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="24dp"
                    android:fontFamily="sans-serif-condensed"
                    android:text="var"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/textView_primary"
                    app:layout_constraintTop_toTopOf="@+id/textView_primary" />

                <TextView
                    android:id="@+id/textView_caption"
                    style="@style/ViewDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="8dp"
                    android:text="Date"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_primary" />

                <TextView
                    android:id="@+id/textView_note"
                    style="@style/Body"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="24dp"
                    android:text="Note"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_caption"
                    tools:visibility="visible" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/textView_sigma"
                    android:layout_width="20dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_24dp"
                    android:layout_marginTop="24dp"
                    android:fontFamily="sans-serif-condensed"
                    android:text="I"
                    android:textColor="@color/colorAccViolet"
                    android:textSize="@dimen/type_heading1"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView_interval"
                    style="@style/Heading1"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="8dp"
                    android:fontFamily="sans-serif"
                    android:text="interval"
                    app:layout_constraintStart_toEndOf="@+id/textView_sigma"
                    app:layout_constraintTop_toTopOf="@+id/textView_sigma" />

                <TextView
                    android:id="@+id/textView_s"
                    android:layout_width="20dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_24dp"
                    android:layout_marginTop="16dp"
                    android:fontFamily="sans-serif-condensed"
                    android:text="s"
                    android:textColor="@color/colorAccBlue"
                    android:textSize="@dimen/type_heading1"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_sigma" />

                <TextView
                    android:id="@+id/textView_distance"
                    style="@style/Heading1"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_8dp"
                    android:fontFamily="sans-serif"
                    android:text="distance"
                    app:layout_constraintStart_toEndOf="@+id/textView_s"
                    app:layout_constraintTop_toTopOf="@+id/textView_s" />

                <TextView
                    android:id="@+id/textView_t"
                    android:layout_width="20dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_24dp"
                    android:layout_marginTop="@dimen/margin_16dp"
                    android:fontFamily="sans-serif-condensed"
                    android:text="t"
                    android:textColor="@color/colorAccTurqoise"
                    android:textSize="@dimen/type_heading1"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_s" />

                <TextView
                    android:id="@+id/textView_time"
                    style="@style/Heading1"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_8dp"
                    android:fontFamily="sans-serif"
                    android:text="time"
                    app:layout_constraintStart_toEndOf="@+id/textView_t"
                    app:layout_constraintTop_toTopOf="@+id/textView_t" />

                <TextView
                    android:id="@+id/textView_v"
                    android:layout_width="20dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_24dp"
                    android:layout_marginTop="@dimen/margin_16dp"
                    android:fontFamily="sans-serif-condensed"
                    android:text="v"
                    android:textColor="@color/colorAccGreen"
                    android:textSize="@dimen/type_heading1"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_t" />

                <TextView
                    android:id="@+id/textView_velocity"
                    style="@style/Heading1"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_8dp"
                    android:fontFamily="sans-serif"
                    android:text="pace"
                    app:layout_constraintStart_toEndOf="@+id/textView_v"
                    app:layout_constraintTop_toTopOf="@+id/textView_v" />

                <TextView
                    android:id="@+id/textView_E"
                    android:layout_width="20dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_24dp"
                    android:layout_marginTop="@dimen/margin_16dp"
                    android:fontFamily="sans-serif-condensed"
                    android:text="E"
                    android:textColor="@color/colorAccYellow"
                    android:textSize="@dimen/type_heading1"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_v" />

                <TextView
                    android:id="@+id/textView_energy"
                    style="@style/Heading1"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_8dp"
                    android:fontFamily="sans-serif"
                    android:text="energy"
                    app:layout_constraintStart_toEndOf="@+id/textView_E"
                    app:layout_constraintTop_toTopOf="@+id/textView_E" />

                <TextView
                    android:id="@+id/textView_P"
                    android:layout_width="20dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_24dp"
                    android:layout_marginTop="@dimen/margin_16dp"
                    android:fontFamily="sans-serif-condensed"
                    android:text="P"
                    android:textColor="@color/colorAccOrange"
                    android:textSize="@dimen/type_heading1"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_E" />

                <TextView
                    android:id="@+id/textView_power"
                    style="@style/Heading1"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_8dp"
                    android:fontFamily="sans-serif"
                    android:text="power"
                    app:layout_constraintStart_toEndOf="@+id/textView_P"
                    app:layout_constraintTop_toTopOf="@+id/textView_P" />

                <TextView
                    android:id="@+id/textView_h"
                    android:layout_width="20dp"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_24dp"
                    android:layout_marginTop="16dp"
                    android:fontFamily="sans-serif-condensed"
                    android:text="h"
                    android:textColor="@color/colorAccRed"
                    android:textSize="@dimen/type_heading1"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_P" />

                <TextView
                    android:id="@+id/textView_elevation"
                    style="@style/Heading1"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:layout_marginStart="@dimen/margin_8dp"
                    android:fontFamily="sans-serif"
                    android:text="elevation"
                    app:layout_constraintStart_toEndOf="@+id/textView_P"
                    app:layout_constraintTop_toTopOf="@+id/textView_h" />

                <View
                    android:id="@+id/divider9"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="24dp"
                    android:background="?android:attr/listDivider"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_elevation"
                    tools:visibility="invisible" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout_mapHolder"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <FrameLayout
                    android:id="@+id/frameLayout_mapFragment"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp"
                    android:background="@drawable/shape_rounded"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                </FrameLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/divider11"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="24dp"
                    android:background="?android:attr/listDivider"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView_id"
                    style="@style/Caption"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="16dp"
                    android:text="#"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider11" />

                <TextView
                    android:id="@+id/textView_type"
                    style="@style/Caption"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:text="Type"
                    app:layout_constraintStart_toEndOf="@+id/textView_id"
                    app:layout_constraintTop_toTopOf="@+id/textView_id" />

                <TextView
                    android:id="@+id/textView_dataSource"
                    style="@style/Caption"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="8dp"
                    android:text="Data source"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_type" />

                <TextView
                    android:id="@+id/textView_recordingMethod"
                    style="@style/Caption"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="24dp"
                    android:text="Recording method"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView_dataSource" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>